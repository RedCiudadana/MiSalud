<div class="container">
  <div class="row">
    <div class="col-12 py-4">
      <h1>Abastecimientos</h1>
      <h2>Trazadores disponibilidad mayor a 1 mes</h2>
      <div class="table-responsive mb-2">
        <table class="table table-sm">
          <thead>
            <tr>
              <th scope="col">
                Nombre
              </th>
              <th scope="col">
                Medicamentos trazadores
              </th>
              <th scope="col">
                Medicamentos quirurgico trazador
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each model as |hospital|}}
              <tr>
                <td>{{ hospital.nombre }}</td>
                <td>{{ hospital.trazadores1MFebrero }}</td>
                <td>{{ hospital.quirurgicoTrazador1MFebrero }}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <h2>Disponibilidad mayor a 1 mes</h2>
      <div class="table-responsive mb-2">
        <table class="table table-sm">
          <thead>
            <tr>
              <th scope="col">
                Nombre
              </th>
              <th scope="col">
                Medicamentos con disponibilidad
              </th>
              <th scope="col">
                Medicamentos quirurgicos con disponibilidad
              </th>
              <th scope="col">
                Reactivos para Laboratorio Clínico
              </th>
              <th scope="col">
                Reactivos para Banco de Sangre
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each model as |hospital|}}
              <tr>
                <td>{{ hospital.nombre }}</td>
                <td>{{ hospital.medicamentos1MFebrero }}</td>
                <td>{{ hospital.quirurgico1MFebrero }}</td>
                <td>{{ hospital.laboratorio1MFebrero }}</td>
                <td>{{ hospital.banco1MFebrero }}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <h2>Disponibilidad mayor a 3 meses</h2>
      <div class="table-responsive mb-2">
        <table class="table table-sm">
          <thead>
            <tr>
              <th scope="col">
                Nombre
              </th>
              <th scope="col">
                Medicamentos
              </th>
              <th scope="col">
                Medicamentos quirurgicos
              </th>
              <th scope="col">
                Reactivos para Laboratorio Clínico
              </th>
              <th scope="col">
                Reactivos para Banco de Sangre
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each model as |hospital|}}
              <tr>
                <td>{{ hospital.nombre }}</td>
                <td>{{ hospital.medicamentos3MFebrero }}</td>
                <td>{{ hospital.quirurgico3MFebrero }}</td>
                <td>{{ hospital.laboratorio3MFebrero }}</td>
                <td>{{ hospital.banco3MFebrero }}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>